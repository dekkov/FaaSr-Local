{
    "ComputeServers": {
      "My_GitHub_Account": {
        "FaaSType": "GitHubActions",
        "UserName": "dekkov",
        "UseSecretStore": false,
        "ActionRepoName": "FaaSr-workflow",
        "Branch": "main",
        "Token": "My_GitHub_Account_TOKEN"
      }
    },
    "DataStores": {
      "My_Minio_Bucket": {
        "Bucket": "hoang",
        "Endpoint": "https://play.min.io",
        "Region": "us-east-1",
        "Writable": "TRUE",
        "AccessKey": "My_Minio_Bucket_ACCESS_KEY",
        "SecretKey": "My_Minio_Bucket_SECRET_KEY"
      }
    },
    "ActionList": {
      "start": {
        "FunctionName": "create_sample_data",
        "FaaSServer": "My_GitHub_Account",
        "Type": "R",
        "Arguments": {
            "folder": "tutorial",
            "output1": "sample1.csv",
            "output2": "sample2.csv"
        },
        "InvokeNext": ["sum"]
      },
      "sum": {
        "FunctionName": "compute_sum",
        "FaaSServer": "My_GitHub_Account",
        "Type": "R",
        "Arguments": {
            "folder": "tutorial",
            "input1": "sample1.csv",
            "input2": "sample2.csv",
            "output": "sum.csv"
        },
        "InvokeNext": []
      }
    },
    "ActionContainers": {
      "start": "ghcr.io/faasr/github-actions-tidyverse",
      "sum": "ghcr.io/faasr/github-actions-tidyverse"
  },
    "FunctionGitRepo": {
        "create_sample_data": "FaaSr/FaaSr-tutorial",
        "compute_sum": "FaaSr/FaaSr-tutorial"
    },
    "FunctionInvoke": "start",
    "InvocationID":"",
    "InvocationIDFromDate": "%Y%m%d%H%M",
    "FaaSrLog": "FaaSrLog",
    "LoggingDataStore": "My_Minio_Bucket",
    "DefaultDataStore": "My_Minio_Bucket",
    "WorkflowName": "project1"
  }